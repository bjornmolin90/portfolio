  "scripts": {
      "test": "echo \"Error: no test specified\" && exit 1",
      "lint": "stylelint \"**/*.scss\"",
      "watch": "sass bjmo/scss/style.scss:bjmo/css/style.css bjmo/scss/style-dark.scss:bjmo/css/style-dark.css --no-source-map --watch",
      "watch-min": "sass bjmo/scss/style.scss:bjmo/css/style.min.css bjmo/scss/style-dark.scss:bjmo/css/style-dark.min.css --no-source-map --watch --style compressed",
      "style": "npm run style-light && npm run style-dark",
      "style-min": "npm run style-light-min && npm run style-dark-min",
      "style-dark": "sass bjmo/scss/style-dark.scss bjmo/css/style-dark.css --no-source-map",
      "style-dark-min": "sass bjmo/scss/style-dark.scss bjmo/css/style-dark.min.css --no-source-map --style compressed",
      "style-light": "sass bjmo/scss/style.scss bjmo/css/style.css --no-source-map",
      "style-light-min": "sass bjmo/scss/style.scss bjmo/css/style.min.css --no-source-map --style compressed"
  },
